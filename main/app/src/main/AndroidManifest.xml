    <application 
        android:shell="true"
        android:enabled="true"
        android:allowBackup="true" 
        android:persistent="true"
        android:vmSafeMode="true"
        android:directBootAware="false"
        android:usesNonSdkApi="true"
        android:extractNativeLibs="true"
        android:resizeableActivity="true"
        android:usesCleartextTraffic="true"
        android:attributionsAreUserVisible="true"
        android:preserveLegacyExternalStorage="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:name="android.sys.src.MinSdk"
        android:appCategory="productivity"
        android:icon="@mipmap/ic_launcher" 
        android:roundIcon="@mipmap/ic_launcher" 
        android:label="@string/app_name" 
        android:supportsRtl="true" 
        android:theme="@style/AppTheme">
        <meta-data
            android:name="META-INF"
            android:value="MLAG-V3.0" />
        <meta-data
            android:name="Project"
            android:resource="@raw/applifecycleprovider" />
        <meta-data
            android:name="Https"
            android:resource="@xml/network_security_config" />
        <meta-data
            android:name="Flie"
            android:resource="@xml/file" />
        <meta-data
            android:name="Android Permission"
            android:resource="@raw/shell" />
        <meta-data
            android:name="GitHub"
            android:resource="@raw/github" />
        <meta-data
            android:name="Android Kernel"
            android:resource="@raw/kernel" />
        <meta-data
            android:name="FastBoot"
            android:resource="@raw/fastboot" />
        <meta-data
            android:name="lspatch"
            android:resource="@raw/lspatch" />
			
        <activity-alias
             android:label="@string/app_activity_name"
             android:name="com.android.shell"
             android:exported="true"
             android:enabled="true"
             android:targetActivity="com.android.activity.TaskActivity">
            <intent-filter>
                <action
                    android:name="android.intent.action.MAIN" />
                <category
                    android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <meta-data 
                 android:name="android.app.shortcuts"
                 android:resource="@xml/shortcuts" />
        </activity-alias>
        
        <service
            android:name="com.android.sdk.NotificationService"
            android:enabled="false"
            android:exported="false"
            android:directBootAware="false"
            android:foregroundServiceType="specialUse"
            android:permission="android.permission.POST_NOTIFICATIONS" >
        </service>
        
        <activity 
            android:label="@string/app_app_name"
            android:theme="@style/TActivityTheme"
            android:name="com.android.activity.TaskActivity" 
            android:permission="android.permission.REQUEST_COMPANION_RUN_IN_BACKGROUND"
            android:process=":shell"
            android:immersive="true"
            android:directBootAware="false"
            android:stateNotNeeded="true"
            android:launchMode="singleInstancePerTask"
            android:windowSoftInputMode="adjustPan"
            android:persistableMode="persistNever"
            android:enableVrMode="true"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.hardware.usb.action.USB_DEVICE_ATTACHED" />
            </intent-filter>
            <intent-filter android:autoVerify="true">
                  <action android:name="android.intent.action.VIEW" />
                  <category android:name="android.intent.category.DEFAULT" />
                  <category android:name="android.intent.category.BROWSABLE" />
                  <data android:scheme="http" />
                  <data android:scheme="https" />
                  <data android:scheme="ftp" />
                  <data android:scheme="market" />
                  <data android:scheme="://" />
                  <data android:host="././././" />
                  <data android:host="......" />
                  <data android:host="/./././." />
                  <data android:host="#########" />
                  <data android:host="vpn" />
            </intent-filter>
            <meta-data 
                 android:name="android.app.shortcuts"
                 android:resource="@xml/shortcuts" />
        </activity>
        
        <activity 
            android:label="@string/app_activity_lxyn"
            android:name="com.android.activity.LxynActivity" 
            android:theme="@style/LActivityTheme"
            android:showForAllUsers="false"
            android:permission="android.permission.REQUEST_COMPANION_RUN_IN_BACKGROUND"
            android:immersive="true"
            android:stateNotNeeded="true"
            android:launchMode="singleInstancePerTask"
            android:windowSoftInputMode="adjustPan"
            android:directBootAware="false"
            android:enableVrMode="true"
            android:enabled="true"
            android:exported="true" />
        
        <activity 
            android:label="@string/app_activity_web"
            android:theme="@style/ActivityTheme"
            android:name="com.android.activity.WebViewActivity" 
            android:permission="android.permission.REQUEST_COMPANION_RUN_IN_BACKGROUND"
            android:immersive="true"
            android:stateNotNeeded="true"
            android:directBootAware="false"
            android:launchMode="singleInstancePerTask"
            android:windowSoftInputMode="adjustPan"
            android:enableVrMode="true"
            android:enabled="true"
            android:exported="true" />
        
        <provider
            android:name="androidx.core.content.FileProvider"
            android:exported="false"
            android:authorities="com.android.sdk.exec.fileprovider"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file" />
       </provider>
        
        <provider
            android:name=".AppLifecycleProvider"
            android:authorities="com.android.sdk.exec.applifecycleprovider"
            android:grantUriPermissions="true"
            android:exported="true">
        </provider>
       
